<AnchorPane prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Dictionary.DictionaryController.MyWordsController">
   <children>
      <ImageView fitHeight="600.0" fitWidth="900.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/MyWords.png" />
         </image>
      </ImageView>
      <ListView fx:id="wordlist" fixedCellSize="0.0" layoutX="439.0" layoutY="207.0" prefHeight="294.0" prefWidth="402.0" styleClass="list-word" stylesheets="@test.css" />
      <Text layoutX="380.0" layoutY="130.0" strokeType="OUTSIDE" strokeWidth="0.0" text="My Words">
         <font>
            <Font name="Playfair Display Bold" size="33.0" />
         </font>
      </Text>
      <TextField fx:id="searchbar" layoutX="66.0" layoutY="287.0" onKeyTyped="#searchMyWord" prefHeight="30.0" prefWidth="187.0" />
   </children>
</AnchorPane>
